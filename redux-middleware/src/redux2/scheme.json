{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [{
        "timer": {
            "value": 0,
            "step": {
                "value": 5,
                "label": 5
            }
        }
    }],
    "required": [
        "timer"
    ],
    "additionalProperties": true,
    "properties": {
        "timer": {
            "$id": "#/properties/timer",
            "type": "object",
            "title": "The timer schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [{
                "value": 0,
                "step": {
                    "value": 5,
                    "label": 5
                }
            }],
            "required": [
                "value",
                "step"
            ],
            "additionalProperties": true,
            "properties": {
                "value": {
                    "$id": "#/properties/timer/properties/value",
                    "type": "integer",
                    "title": "The value schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        0
                    ]
                },
                "step": {
                    "$id": "#/properties/timer/properties/step",
                    "type": "object",
                    "title": "The step schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": {},
                    "examples": [{
                        "value": 5,
                        "label": 5
                    }],
                    "required": [
                        "value",
                        "label"
                    ],
                    "additionalProperties": true,
                    "properties": {
                        "value": {
                            "$id": "#/properties/timer/properties/step/properties/value",
                            "type": "integer",
                            "title": "The value schema",
                            "description": "An explanation about the purpose of this instance.",
                            "default": 0,
                            "examples": [
                                5
                            ]
                        },
                        "label": {
                            "$id": "#/properties/timer/properties/step/properties/label",
                            "type": "integer",
                            "title": "The label schema",
                            "description": "An explanation about the purpose of this instance.",
                            "default": 0,
                            "examples": [
                                5
                            ]
                        }
                    }
                }
            }
        }
    }
}